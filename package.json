    "babel-plugin-transform-require-context": "^0.1.1",
    "babel-preset-react-app": "^9.1.2",
    "commitizen": "^4.1.2",
    "css-loader": "^3.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^1.7.1",
