    "webpack-merge": "^4.2.1",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "dt-python-parser": "0.8.0",
    "dt-react-component": "2.0.0",
    "dt-sql-parser": "3.0.5",
    "lodash": "^4.17.19",
    "monaco-editor": "^0.30.1"
  }
}
